{
	"name": "ModernDeck",
	"productName": "ModernDeck",
	"version": "7.5.0",
	"description": "ModernDeck",
	"copyright": "Copyright 2014-2019 dangeredwolf, et al. Released under the MIT license.",
	"homepage": "https://github.com/dangeredwolf/ModernDeck",
	"bugs": {
		"url": "https://github.com/dangeredwolf/ModernDeck/issues"
	},
	"author": "dangeredwolf",
	"license": "MIT",
	"main": "main.js",
	"scripts": {
		"pack": "electron-builder --dir",
		"dist": "electron-builder . --publish always",
		"distAll": "electron-builder -mwl --publish always",
		"build": "electron-builder . --config electron-builder.json",
		"buildAll": "electron-builder -mwl --config electron-builder.json",
		"buildWindows": "electron-builder -w --config electron-builder.json",
		"buildMac": "electron-builder -m --config electron-builder.json",
		"buildLinux": "electron-builder -l --config electron-builder.json",
		"efMake": "electron-forge make --arch x64",
		"start": "electron .",
		"inspect": "electron . --inspect",
		"release": "build"
	},
	"config": {
		"forge": "./build/config.forge.js"
	},
	"repository": "https://github.com/dangeredwolf/ModernDeck",
	"keywords": [
		"TweetDeck",
		"ModernDeck",
		"Modern",
		"Tweet",
		"Deck",
		"Material",
		"Design"
	],
	"dependencies": {
		"asar": "^2.0.3",
		"electron-log": "^4.0.3",
		"electron-store": "^5.1.0",
		"electron-updater": "^4.2.0",
		"file-type": "^13.0.3",
		"got": "^10.2.2",
		"through2": "^3.0.1"
	},
	"devDependencies": {
		"@node-minify/core": "^5.2.0",
		"@node-minify/csso": "^5.2.0",
		"@node-minify/uglify-es": "^5.2.0",
		"adm-zip": "^0.4.13",
		"electron": "^6.1.7",
		"electron-builder": "^21.2.0",
		"electron-notarize": "^0.2.1",
		"fs-extra": "^8.1.0",
		"klaw": "^3.0.0",
		"stylelint": "^12.0.1",
		"stylelint-config-recommended": "^3.0.0"
	},
	"stylelint": {
		"extends": "stylelint-config-recommended"
	}
}
